{
	"name" : "sasedev/commons-postgresql-bundle",
	"description" : "Registering custom Doctrine's PostgreSQL types",
	"keywords" : [
		"symfony2",
		"postgresql",
		"doctrine",
		"dbal"
	],
	"type" : "symfony-bundle",
	"license" : "MIT",
	"authors" : [{
			"name" : "Salah Abdelkader Seif Eddine",
			"email" : "seif.salah@gmail.com"
		}
	],
	"autoload" : {
		"psr-0" : {
			"SASEdev" : "src/"
		}
	},
	"require" : {
		"php" : ">=5.3.3",
		"symfony/symfony" : "~2.3",
		"doctrine/orm" : "~2.2,>=2.2.3",
		"sasedev/doctrine-dbal-pgsql-types" : "dev-master"
	},
	"config" : {
		"bin-dir" : "bin",
		"process-timeout" : 1200
	},
	"extra" : {
		"branch-alias" : {
			"dev-master" : "1.0-dev"
		}
	},
	"minumum-stablility" : "stable"
}